{
  "expo": {
    "name": "Te Juego una Pichanga",
    "slug": "pichanga-client",
    "version": "0.1.2",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "pichanga",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pichanga.client",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAtJRhOU-bgDB_t8b1o0GoWENJTs4V2jD8"
        }
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "847dd712-8ff7-4749-80cf-94084cf430ca"
      }
    }
  }
}
