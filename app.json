{
  "expo": {
    "name": "Te Juego una Pichanga",
    "slug": "pichanga-client",
    "owner": "tjupichanga",
    "version": "0.3.3",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "pichanga",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pichanga.client",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA8d4EpM55qBhh8iQYm1nnLWbJTYMl4NYc"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir a $(PRODUCT_NAME) acceder a tu cámara y fotos."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir a $(PRODUCT_NAME) usar tu ubicación."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "847dd712-8ff7-4749-80cf-94084cf430ca"
      }
    }
  }
}
